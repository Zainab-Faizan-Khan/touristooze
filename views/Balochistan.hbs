    <script>
  function enableScrolling(){
    window.onscroll=function(){};
    }
      function disableScrolling(){
          var x=window.scrollX;
          var y=window.scrollY;
          window.onscroll=function(){window.scrollTo(x, y);};
      }
      document.querySelector("#close2").addEventListener("click", function(){
            document.querySelector("#filterportion").style.display="none";
            enableScrolling()
            })
      document.querySelector("#cancelBtn").addEventListener("click", function(){
            document.querySelector("#filterportion").style.display="none";
            enableScrolling()
            })
      document.querySelector("#applyBtn").addEventListener("click", function(){
            document.querySelector("#filterportion").style.display="none";
            enableScrolling()
            })
      document.querySelector("#fbtn").addEventListener("click", function(){
            document.querySelector("#filterportion").style.display="block";
            disableScrolling()})
  </script>
  
